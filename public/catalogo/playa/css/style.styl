@font-face
  font-family: 'iconos'
  src: url('fonts/iconos.eot')
  src: url('fonts/iconos.eot') format('embedded-opentype'), url('fonts/iconos.woff') format('woff'), url('fonts/iconos.ttf') format('truetype'), url('fonts/iconos.svg') format('svg')
  font-weight: normal
  font-style: normal

[class^="icon-"],
[class*=" icon-"]
  font-family: 'iconos'
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1
  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

.icon-oficina:before
  content: "\e600"
  color: #f3af32
  

.icon-phone:before
  content: "\e603"
  color: #f3af32
  

.icon-mobile:before
  content: "\e601"
  color: #f3af32
  

.icon-point-up:before
  content: "\e602"
  color: #f3af32
  

